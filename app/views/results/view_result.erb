<center><h1> <%= @category.name %> Results </h1>

<% @results.each do |key, value| %>
  rank:&emsp;
  <% @count += 1 %>
    <%  if value != @last_nominee %>
      <% @last_nominee = value %>
      <%= @rank += @count %>
      <% @count = 0 %>
   <% else  %>
       <%= @rank %>
   <% end %>
 &emsp;&emsp;|&emsp;&emsp; 
 winner id:<%= key %> 
 &emsp;&emsp;|&emsp;&emsp;
 #of times won:<%= value %>
  <br>
<% end %>

<%= "-----------------------------------------------------------" %>
<br>
<% @results_breakdown.each do |key, value| %>
<%= key %> &emsp;&emsp;|&emsp;&emsp;<%= value %><br>
<% end %>

</center>